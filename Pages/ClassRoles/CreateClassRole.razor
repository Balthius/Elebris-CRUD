@page "/ClassRole/Create"
@inject NavigationManager UriHelper
@inject IElebrisClassData classData
@attribute [Authorize(Roles="admin, superuser")]
<h3>Create</h3>
<ClassRoleForm ButtonText="Create" SelectedRole="@SelectedRole"
               OnValidSubmit="@CreateNewRole" />
@code {
    ClassRoleModel SelectedRole = new ClassRoleModel();

    protected async override Task OnInitializedAsync()
    {
    }

    protected async override Task OnParametersSetAsync()
    {

    }
    async Task CreateNewRole()
    {

        int id = await classData.CreateElebrisRole(SelectedRole);
        NavigateToFetchData();
    }
    public void NavigateToFetchData()
    {
        UriHelper.NavigateTo("/ClassRole/FetchData");
    }
}